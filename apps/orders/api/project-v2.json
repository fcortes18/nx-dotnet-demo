{
  "name": "orders-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/orders/api",
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "{workspaceRoot}/dist/apps/orders/api",
        "{workspaceRoot}/dist/intermediates/apps/orders/api"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "serve": {
      "executor": "@nx-dotnet/core:serve",
      "options": {
        "configuration": "Debug"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
    },
    "swagger": {
      "executor": "@nx-dotnet/core:update-swagger",
      "outputs": [
        "options.output"
      ],
      "options": {
        "output": "libs/generated/orders-api-def/v2/swagger.json",
        "swaggerDoc": "v2"
      },
      "dependsOn": [
        "build"
      ]
    }
  },
  "tags": [
    "orders"
  ]
}
